package java_project_21_02_2024;

public class Secondclass extends Arrays_class
{
	public Secondclass()
	{
        super();
        this.arr = new int[10];
    }

    public Secondclass(int size)
    {
        super(size);
        this.arr = new int[size];
    }
	
	public int element(int index)
	{
		if (index <= arr.length - 1)
			return arr[index];
		else
		{
			System.out.print("Wrong index, returned value is ");
			return 0;
		}
	}
}
